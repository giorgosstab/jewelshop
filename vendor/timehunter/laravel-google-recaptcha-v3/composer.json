{
  "name": "timehunter/laravel-google-recaptcha-v3",
  "description": "Laravel 5.x library for reCAPTCHA v3",
  "license": "MIT",
  "authors": [
    {
      "name": "Ryan Deng",
      "email": "ryandadeng@gmail.com",
      "homepage": "https://ryandeng.com"
    }
  ],
  "homepage": "https://github.com/RyanDaDeng/laravel-google-recaptcha-v3",
  "keywords": [
    "laravel recaptcha",
    "googlerecaptcha",
    "google recaptcha",
    "google recaptcha v3",
    "recaptcha v3",
    "laravel google recaptcha v3",
    "laravel google recaptcha",
    "recaptcha"
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "~3.0",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "TimeHunter\\LaravelGoogleCaptchaV3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TimeHunter\\LaravelGoogleCaptchaV3\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TimeHunter\\LaravelGoogleCaptchaV3\\Providers\\GoogleReCaptchaV3ServiceProvider"
      ],
      "aliases": {
        "GoogleReCaptchaV3": "TimeHunter\\LaravelGoogleCaptchaV3\\Facades\\GoogleReCaptchaV3"
      }
    }
  }
}
